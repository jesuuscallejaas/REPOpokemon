                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform x:Name="TransformacionWartortle"  X="0" Y="0"/>
                </TransformGroup>
            </Canvas.RenderTransform>

