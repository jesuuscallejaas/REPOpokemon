<UserControl
    x:Class="IPOkemonApp.WartortleAAA"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Wartortle"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">
    
    <UserControl.Resources>



        <Storyboard x:Name="AtaqueDebil">

            <!-- Golpe de cola hacia la derecha -->
            <DoubleAnimation Storyboard.TargetName="RotarCola"
                     Storyboard.TargetProperty="Angle"
                     From="0" To="35"
                     Duration="0:0:0.45" AutoReverse="True"/>

            <!-- Efecto de sacudida vertical (impacto) -->
            <DoubleAnimation Storyboard.TargetName="TransformacionWartortle"
                     Storyboard.TargetProperty="Y"
                     From="0" To="-20"
                     Duration="0:0:0.10" AutoReverse="True" BeginTime="0:0:0.1"/>

            <!-- Brillo en la cola -->
            <ColorAnimation Storyboard.TargetName="BrilloCola"
                    Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                    To="White" Duration="0:0:0.15" AutoReverse="True" />
        </Storyboard>

        <Storyboard x:Name="IdleWartortle">
            <!-- Mostrar todo el personaje -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="WartortleCanvas"
                                   Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                    <DiscreteObjectKeyFrame.Value>Visible</DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
            </ObjectAnimationUsingKeyFrames>

            <!-- Restaurar posición (por si se había movido) -->
            <DoubleAnimation Storyboard.TargetName="TransformacionWartortle"
                     Storyboard.TargetProperty="X"
                     To="0" Duration="0:0:0.1" />
            <DoubleAnimation Storyboard.TargetName="TransformacionWartortle"
                     Storyboard.TargetProperty="Y"
                     To="0" Duration="0:0:0.1" />

            <!-- Restaurar opacidad completa por si estaba atenuado -->
            <DoubleAnimation Storyboard.TargetName="WartortleCanvas"
                     Storyboard.TargetProperty="Opacity"
                     To="1" Duration="0:0:0.1" />
        </Storyboard>



        <Storyboard x:Name="Descansar">

            <!-- Ocultar los ojos normales -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="OjoIzq" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0.5" Value="Collapsed"/>
                <DiscreteObjectKeyFrame KeyTime="0:0:3.0" Value="Visible"/>
            </ObjectAnimationUsingKeyFrames>

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="OjoDer" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0.5" Value="Collapsed"/>
                <DiscreteObjectKeyFrame KeyTime="0:0:3.0" Value="Visible"/>
            </ObjectAnimationUsingKeyFrames>

            <!-- Mostrar la imagen de ojos dormidos -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="OjosDormidos" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0.5" Value="Visible"/>
                <DiscreteObjectKeyFrame KeyTime="0:0:3.0" Value="Collapsed"/>
            </ObjectAnimationUsingKeyFrames>

            <!-- Mostrar el gif Zzz -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="GifDormir" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0.5" Value="Visible"/>
                <DiscreteObjectKeyFrame KeyTime="0:0:3.0" Value="Collapsed"/>
            </ObjectAnimationUsingKeyFrames>

        </Storyboard>


        <Storyboard x:Name="KO">

            <!-- Caída del cuerpo en eje Y -->
            <DoubleAnimation
                Storyboard.TargetName="TransformacionWartortle"
                Storyboard.TargetProperty="Y"
                To="80"
                Duration="0:0:0.5"
                AutoReverse="False" />

            <!-- Temblor en eje X -->
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="TransformacionWartortle"
                                   Storyboard.TargetProperty="X">
                <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="-10"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="-5"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="5"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

            <!-- Mostrar imagen KO -->
            <DoubleAnimation
                Storyboard.TargetName="ImagenKO"
                Storyboard.TargetProperty="Opacity"
                From="0" To="1"
                BeginTime="0:0:0.3"
                Duration="0:0:0.4" />

        </Storyboard>


        <Storyboard x:Name="Herido">

            <!-- Temblor -->
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="TransformacionWartortle"
                                   Storyboard.TargetProperty="X">
                <EasingDoubleKeyFrame KeyTime="0:0:0.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="-5"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="5"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="-5"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="5"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

            <!-- Ojo morado -->
            <ColorAnimation Storyboard.TargetName="IrisIzq"
                    Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                    To="#7E4C9B" Duration="0:0:0.3"/>

            <!-- Ocultar diente -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Diente1" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Collapsed"/>
            </ObjectAnimationUsingKeyFrames>

            <!-- Mostrar herida visual -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeridaVisual" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible"/>

            </ObjectAnimationUsingKeyFrames>

            <!-- Mostrar herida visual 2 -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeridaVisual2" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible"/>

            </ObjectAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Name="Cansado">
            <!-- Mostrar sudor -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Sudor" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible"/>
            </ObjectAnimationUsingKeyFrames>

            <!-- Ojos tristes (baja opacidad) -->
            <DoubleAnimation Storyboard.TargetName="IrisIzq" Storyboard.TargetProperty="Opacity"
                     To="0.5" Duration="0:0:0.2"/>
            <DoubleAnimation Storyboard.TargetName="IrisDer" Storyboard.TargetProperty="Opacity"
                     To="0.5" Duration="0:0:0.2"/>

            <!-- Bajada leve del cuerpo por fatiga -->
            <DoubleAnimation Storyboard.TargetName="TransformacionWartortle"
                     Storyboard.TargetProperty="Y"
                     To="20" Duration="0:0:0.2"/>
        </Storyboard>

        <Storyboard x:Name="Defensa">

            <!-- Mostrar escudo -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="EscudoGigante" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible"/>
            </ObjectAnimationUsingKeyFrames>

            <!-- Mostrar campo de fuerza -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="CampoDeFuerza" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible"/>
            </ObjectAnimationUsingKeyFrames>

            <!-- Opacidad del campo de fuerza (pulsante) -->
            <DoubleAnimation Storyboard.TargetName="CampoDeFuerza"
                     Storyboard.TargetProperty="Opacity"
                     From="0.3" To="0.8"
                     Duration="0:0:0.5"
                     AutoReverse="True"
                     RepeatBehavior="Forever"/>

            <!-- Vibración/expansión del campo de fuerza -->
            <DoubleAnimation Storyboard.TargetName="EscalaCampo"
                     Storyboard.TargetProperty="ScaleX"
                     From="1" To="1.1"
                     Duration="0:0:0.5"
                     AutoReverse="True"
                     RepeatBehavior="Forever"/>
            <DoubleAnimation Storyboard.TargetName="EscalaCampo"
                     Storyboard.TargetProperty="ScaleY"
                     From="1" To="1.1"
                     Duration="0:0:0.5"
                     AutoReverse="True"
                     RepeatBehavior="Forever"/>
        </Storyboard>

        <Storyboard x:Name="AtaqueFuerte">

            <!-- Visibilidad del chorro (Mostrar y Ocultar) -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ChorroAgua"
                                   Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible"/>
                <DiscreteObjectKeyFrame KeyTime="0:0:4" Value="Collapsed"/>
            </ObjectAnimationUsingKeyFrames>

            <!-- Movimiento hacia adelante del chorro (5 segundos) -->
            <DoubleAnimation Storyboard.TargetName="ChorroAguaTransform"
                     Storyboard.TargetProperty="X"
                     From="-350" To="-600"
                     Duration="0:0:4" />

            <!-- Efecto de sacudida inicial del cuerpo al lanzar -->
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="TransformacionWartortle"
                                   Storyboard.TargetProperty="Y">
                <EasingDoubleKeyFrame KeyTime="0:0:0.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="-10"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

        </Storyboard>




    </UserControl.Resources>


    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="21*"/>
            <ColumnDefinition Width="257*"/>
            <ColumnDefinition Width="21*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="70*"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>

        <MediaElement x:Name="SonidoKO"
              Source="Assets/ko.mp3"
              AutoPlay="False"
              Volume="0.2"
              Visibility="Collapsed"/>

        <MediaPlayerElement x:Name="SnorePlayer"
                    Source="ms-appx:///Assets/Ronquido.mp3"
                    AutoPlay="False"
                    Visibility="Collapsed" />

        <MediaElement x:Name="SonidoDebil" 
                      Source="Assets/debil.mp3"
                      AutoPlay="False"
                      Volume="0.1"
                      Visibility="Collapsed"/>

        <MediaElement x:Name="SonidoHerido"
              Source="Assets/herido.mp3"
              AutoPlay="False"
              Visibility="Collapsed"
              Volume="0.1"/>

        <MediaElement x:Name="SonidoEscudo"
              AutoPlay="False"
              Volume="0.6"
              Visibility="Collapsed"
              Source="Assets/escudo.mp3"/>

        <MediaElement x:Name="SonidoFuerte"
              Source="ms-appx:///Assets/fuerte.mp3"
              Volume="0.3"
              AutoPlay="False"/>




        <Image x:Name="fondoPokemon" Grid.Column="0" Grid.Row="0" Grid.RowSpan="4" Grid.ColumnSpan="4" Source="/AssetsWartortleAAA/fondo.png" Stretch="UniformToFill" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Image x:Name="pocenergia"  Source="/AssetsWartortleAAA/yellow-potion.png" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="2" Grid.Row="1" Tapped="AumentarEnergia"/>
        <Image x:Name="salud"  Source="/AssetsWartortleAAA/corazon.png" VerticalAlignment="Center" HorizontalAlignment="Center" />
        <Image x:Name="energiaIcono"  Source="/AssetsWartortleAAA/energia.png" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Row="1" />
        <ProgressBar x:Name="ProgressBarVida" Grid.Column="1" Margin="5,5,5,5" VerticalAlignment="Stretch" Background="#4CE60A0A" Foreground="#FFE60A0A" Value="100"/>
        <ProgressBar x:Name="ProgressBarEner" Grid.Row="1" Grid.Column="1" Margin="5,5,5,5" VerticalAlignment="Stretch" Background="#4CFFF000" Foreground="#FFFFF900" Value="100" />
        <Image x:Name="vidaPot"  Source="/AssetsWartortleAAA/red-potion.png" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="2" Tapped="AumentarVida"/>
      


