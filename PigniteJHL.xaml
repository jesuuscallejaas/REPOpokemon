<UserControl
    x:Class="IPOkemonApp.PigniteJHL"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Practica1IPO2"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="886"
    d:DesignWidth="1151">

    <UserControl.Resources>
        <Storyboard x:Name="AtaqueFuerte">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="canvasPignite" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.Rotation)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:00.1000000" Value="-90"/>
            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="canvasPignite" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)">
                <EasingDoubleKeyFrame KeyTime="00:00:00.5" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01.0" Value="-400"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimation Storyboard.TargetName="llamarada" Storyboard.TargetProperty="Opacity"
                         From="0" To="0.7" Duration="0:0:0.1" BeginTime="0:0:0.1"/>
        </Storyboard>

        <Storyboard x:Name="RegresarAtaqueFuerte">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="canvasPignite" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.Rotation)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="-90"/>
                <EasingDoubleKeyFrame KeyTime="00:00:00.5" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="canvasPignite" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="-400"/>
                <EasingDoubleKeyFrame KeyTime="00:00:00.5" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimation Storyboard.TargetName="llamarada" Storyboard.TargetProperty="Opacity"
                         From="0.7" To="0" Duration="0:0:0.1" BeginTime="0:0:0.1"/>
        </Storyboard>
        
        <Storyboard x:Name="MostrarEscudo">
            <DoubleAnimation Storyboard.TargetName="escudo" Storyboard.TargetProperty="Opacity"
                         From="0" To="0.6" Duration="0:0:0.5" />

            <DoubleAnimation Storyboard.TargetName="CanvasBrazoIzq" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)"
                         From="0" To="15" Duration="0:0:0.5" />

            <DoubleAnimation Storyboard.TargetName="CanvasBrazoDch" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)"
                         From="0" To="15" Duration="0:0:0.5" />

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="escudo" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="00:00:00" Value="Visible"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>

        <Storyboard x:Name="OcultarEscudo">
            <DoubleAnimation Storyboard.TargetName="escudo" Storyboard.TargetProperty="Opacity"
                         From="0.6" To="0" Duration="0:0:0.5" />

            <DoubleAnimation Storyboard.TargetName="CanvasBrazoIzq" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)"
                         From="15" To="0" Duration="0:0:0.5" />

            <DoubleAnimation Storyboard.TargetName="CanvasBrazoDch" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)"
                         From="15" To="0" Duration="0:0:0.5" />
        </Storyboard>

        <Storyboard x:Name="AtaqueDebil">
            <DoubleAnimation Storyboard.TargetName="canvasPignite"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.Rotation)"
                         From="0" To="-180"
                         Duration="0:0:0.7"
                         AutoReverse="False"/>
        </Storyboard>

        <Storyboard x:Name="MovimientoHaciaAbajo">
            <DoubleAnimation Storyboard.TargetName="canvasPignite"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)"
                         From="0" To="50"
                         Duration="0:0:0.3"
                         AutoReverse="False" />

            <DoubleAnimation Storyboard.TargetName="llamaradaSuelo"
                         Storyboard.TargetProperty="Opacity"
                         From="0" To="1"
                         Duration="0:0:0.5"
                         BeginTime="0:0:0.7" />
        </Storyboard>

        <Storyboard x:Name="RegresoAtaqueDebil">
            <DoubleAnimation Storyboard.TargetName="canvasPignite"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.Rotation)"
                         From="-180" To="0"
                         Duration="0:0:1"
                         AutoReverse="False"/>

            <DoubleAnimation Storyboard.TargetName="canvasPignite"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)"
                         From="50" To="0"
                         Duration="0:0:1"
                         AutoReverse="False"/>

            <DoubleAnimation Storyboard.TargetName="llamaradaSuelo"
                         Storyboard.TargetProperty="Opacity"
                         From="1" To="0"
                         Duration="0:0:0.5" />
        </Storyboard>

        <Storyboard x:Name="AnimacionDescanso" Duration="0:0:5">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="CanvasBrazoIzq" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:00.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:04.0" Value="0"/>

            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="CanvasBrazoDch" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:00.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:04.0" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="canvasCola" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:00.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03.0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03.5" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:04.0" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="particulas" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02.5" Value="-50"/>
                <EasingDoubleKeyFrame KeyTime="00:00:05" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="particulas" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="00:00:00" Value="Visible"/>
            </ObjectAnimationUsingKeyFrames>

            <DoubleAnimation Storyboard.TargetName="particulas"
                Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="0:0:0.5" />
        </Storyboard>

        <Storyboard x:Name="DesaparicionParticulas" Duration="0:0:05">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="particulas" Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="00:00:05" Value="Collapsed"/>
            </ObjectAnimationUsingKeyFrames>

            <DoubleAnimation Storyboard.TargetName="particulas"
             Storyboard.TargetProperty="Opacity"
             From="1" To="0"
             Duration="0:0:0.5" />
        </Storyboard>

        <Storyboard x:Name="AnimacionIdle" RepeatBehavior="Forever">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="CanvasBrazoIzq" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:04" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="CanvasBrazoDch" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:04" Value="0"/>
            </DoubleAnimationUsingKeyFrames>

            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="canvasCola" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:02" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="00:00:03" Value="10"/>
                <EasingDoubleKeyFrame KeyTime="00:00:04" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </UserControl.Resources>

