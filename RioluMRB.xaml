        <Storyboard x:Key="EscudoAnimation">
            <!-- Cambiar visibilidad del escudo a Visible -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="escudo"
                                   Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible" />
                <!-- Aparece inmediatamente -->
                <DiscreteObjectKeyFrame KeyTime="0:0:20" Value="Collapsed" />
                <!-- Desaparece después de 5 segundos -->
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>


        <Storyboard x:Key="DerrotadoAnimation">
            <!-- Parpadeo: Alternar visibilidad entre visible e invisible -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Pokemon"
                                   Storyboard.TargetProperty="Visibility"
                                   RepeatBehavior="4x">
                <DiscreteObjectKeyFrame KeyTime="0:0:0.3" Value="Collapsed" />
                <DiscreteObjectKeyFrame KeyTime="0:0:0.4" Value="Visible" />
            </ObjectAnimationUsingKeyFrames>

            <!-- Reducción gradual del tamaño (escalado) -->
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="ScaleX"
                     From="1" To="0"
                     Duration="0:0:2" />
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="ScaleY"
                     From="1" To="0"
                     Duration="0:0:2" />

            <!-- Gradual desaparición (opacidad a 0) -->
            <DoubleAnimation Storyboard.TargetName="Pokemon"
                     Storyboard.TargetProperty="Opacity"
                     From="1" To="0"
                     Duration="0:0:2" />
        </Storyboard>

        <Storyboard x:Key="AtaqueDebilAnimation">
            <!-- Movimiento hacia adelante -->
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="TranslateX"
                     From="0" To="20"
                     Duration="0:0:0.1"
                     AutoReverse="True" />

            <!-- Destello de opacidad -->
            <DoubleAnimation Storyboard.TargetName="Pokemon"
                     Storyboard.TargetProperty="Opacity"
                     From="1" To="0.5"
                     Duration="0:0:0.3"
                     AutoReverse="True"
                     RepeatBehavior="1x" />

            <!-- Escalado ligero (pequeño aumento y regreso a su tamaño) -->
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="ScaleX"
                     From="1" To="1.1"
                     Duration="0:0:0.1"
                     AutoReverse="True" />

            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="ScaleY"
                     From="1" To="1.1"
                     Duration="0:0:0.3"
                     AutoReverse="True" />
            <!-- Aparición del destello -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="destello"
                                   Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0.2" Value="Visible" />
                <DiscreteObjectKeyFrame KeyTime="0:0:0.4" Value="Collapsed" />
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>

        <Storyboard x:Key="AtaqueFuerteAnimation">
            <!-- Temblores en el eje vertical (arriba y abajo) -->
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="PokemonTransform"
                                    Storyboard.TargetProperty="TranslateY">
                <EasingDoubleKeyFrame KeyTime="0:0:0.0" Value="0" />
                <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="-10" />
                <!-- Movimiento hacia arriba -->
                <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="10" />
                <!-- Movimiento hacia abajo -->
                <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="-10" />
                <EasingDoubleKeyFrame KeyTime="0:0:1.2" Value="10" />
                <EasingDoubleKeyFrame KeyTime="0:0:1.5" Value="-10" />
                <EasingDoubleKeyFrame KeyTime="0:0:1.8" Value="10" />
                <EasingDoubleKeyFrame KeyTime="0:0:2.1" Value="0" />
                <!-- Regreso al punto original -->
            </DoubleAnimationUsingKeyFrames>

            <!-- Impulso hacia adelante (eje X positivo) -->
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="TranslateX"
                     From="0" To="-200" 
                     BeginTime="0:0:2.1"
                     Duration="0:0:0.5"
                     AutoReverse="True" />

            <!-- Escalado hacia adelante (aumenta tamaño durante el impulso) -->
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="ScaleX"
                     From="1" To="1.3"
                     BeginTime="0:0:2.1"
                     Duration="0:0:0.5"
                     AutoReverse="True" />
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="ScaleY"
                     From="1" To="1.3"
                     BeginTime="0:0:2.1"
                     Duration="0:0:0.5"
                     AutoReverse="True" />

            <!-- Aparición de la explosión justo antes del final de la animación -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="explosion"
                                   Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:2.6" Value="Visible" />
                <!-- Aparece en el segundo 2.4 -->
                <DiscreteObjectKeyFrame KeyTime="0:0:2.8" Value="Collapsed" />
                <!-- Desaparece en el segundo 2.6 -->
            </ObjectAnimationUsingKeyFrames>


        </Storyboard>

        <Storyboard x:Key="Defensa">
            <!-- Cambiar visibilidad del escudo a Visible -->
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="escudo"
                           Storyboard.TargetProperty="Visibility">
                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible" />
            </ObjectAnimationUsingKeyFrames>

            <!-- Hacer que el escudo se vuelva visible gradualmente -->
            <DoubleAnimation Storyboard.TargetName="escudo"
             Storyboard.TargetProperty="Opacity"
             From="0" To="1"
             Duration="0:0:1" />
            <!-- Mover el Pokémon ligeramente hacia la izquierda -->
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
             Storyboard.TargetProperty="TranslateX"
             From="0" To="-50" 
        Duration="0:0:0.5" />


        </Storyboard>

        <Storyboard x:Key="DescansoAnimation">
            <!-- Expansión y contracción en el eje horizontal (ancho) -->
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
                     Storyboard.TargetProperty="ScaleX"
                     From="1" To="1.05"
                     Duration="0:0:2"
                     AutoReverse="True"
                     RepeatBehavior="3x" />

            <!-- Expansión y contracción en el eje vertical (alto) -->
            <DoubleAnimation Storyboard.TargetName="PokemonTransform"
